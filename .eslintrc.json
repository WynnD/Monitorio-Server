{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "semi": ["error", "always"],
      "space-before-function-paren": ["error", "never"],
      "no-console": ["off"]
    },
    "plugins": [
        "standard",
        "promise",
        "react",
        "node"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    }
}
